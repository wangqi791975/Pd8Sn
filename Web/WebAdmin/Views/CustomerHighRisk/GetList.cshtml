@{
    Layout = null;
}
@using Com.Panduo.Service
@using Com.Panduo.Service.Customer
@model PageData<CustomerHighRisk>
<table class="table table-striped table-bordered" data-delaction="@Url.Content("~/CustomerHighRisk/Delete")">
    <thead>
        <tr>
            <th width="30%">客户邮箱</th>
            <th width="20%">添加时间</th>
            <th width="30%">操作人</th>
            <th width="20%">操作</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model.Data)
        {
            <tr>
                <td>@customer.CustomerEmail</td>
                <td>@customer.DateCreated</td>
                <td>@customer.AdminEmail</td>
                <td>
                    <a class="btn btn-mini btn-danger btn-ajax-delete" data-id="@customer.CustomerId"><i class="icon-trash icon-white"></i>删除</a>
                </td>
            </tr>
        }
    </tbody>
</table>
@Html.Partial("~/Views/Shared/Partial/_Page_Bottom.cshtml", @Model.Pager)
